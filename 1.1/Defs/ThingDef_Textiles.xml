<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="ResourceBase">
    <defName>BoiledLeather</defName>
    <label>boiled leather</label>
    <description>Decently high quality leather that has been heavily treated, boiled, and reinforced.  Not good for warm or cool clothing, but sturdy and resistant to blows.  Hard to work with, however.</description>
    <graphicData>
      <texPath>Things/Resource/BoiledLeather</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
		<MaxHitPoints>100</MaxHitPoints>
      <MarketValue>2.5</MarketValue>
      <Mass>0.038</Mass>
      <Flammability>.8</Flammability>
			<DeteriorationRate>.8</DeteriorationRate>
		</statBases>
    <stuffProps>
      <categories>
        <li>Leathery</li>
      </categories>
      <commonality>.5</commonality>
      <color>(18,72,56)</color>
      <statOffsets>
        <ArmorRating_Blunt>0.03</ArmorRating_Blunt>
        <ArmorRating_Sharp>0.03</ArmorRating_Sharp>
        <ArmorRating_Heat>0.03</ArmorRating_Heat>
      </statOffsets>
      <statFactors>
        <WorkToMake>1.3</WorkToMake>
        <ArmorRating_Blunt>1.6</ArmorRating_Blunt>
        <ArmorRating_Sharp>1.7</ArmorRating_Sharp>
        <ArmorRating_Heat>1.7</ArmorRating_Heat>
        <Insulation_Cold>.7</Insulation_Cold>
        <Insulation_Heat>.7</Insulation_Heat>
      </statFactors>
    </stuffProps>
    <thingCategories>
      <li>Textiles</li>
    </thingCategories>
  </ThingDef>
   
</Defs>